Int dodaj(int a, int b)
{
    return a + b;
}

# Argumenty przekazywane przez wartosc
Int dodaj5(int a)
{
    a = a + 5;
    return a;
}

Int a = 2;
Int b = dodaj(a, 4);
Int c = dodaj5(a);

println(a); # 2
println(b); # 6
println(c); # 7


# Rekurencja

Int silnia(int n)
{
    if(n <= 1) { return 1; }
    return n*silnia(n-1);
}

print(silnia(5)); # 120